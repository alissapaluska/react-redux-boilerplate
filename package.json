{
    "name": "react-redux-boilerplate",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "update:packages": "node ./internals/scripts/wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "analyze:clean": "rimraf stats.json",
        "analyze": "node ./internals/scripts/analyze.js",
        "extract-intl": "node ./internals/scripts/extract-intl.js",
        "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
        "lint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
        "lint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts -- . ",
        "pretest": "npm run test:clean && npm run lint",
        "test:clean": "rimraf ./coverage",
        "test:watch": "cross-env NODE_ENV=test jest --watchAll",
        "prettify": "prettier --write"
    },
    "dependencies": {
        "@babel/polyfill": "7.4.3",
        "@material-ui/core": "*",
        "@material-ui/icons": "*",
        "axios": "*",
        "dotenv": "*",
        "history": "4.9.0",
        "humps": "*",
        "immer": "*",
        "lodash": "*",
        "normalizr": "*",
        "prop-types": "*",
        "react": "*",
        "react-dom": "*",
        "react-helmet": "6.0.0-beta",
        "react-intl": "2.8.0",
        "react-redux": "*",
        "react-router-dom": "*",
        "react-scripts": "3.4.1",
        "redux": "*",
        "redux-thunk": "*",
        "redux-undo": "*",
        "reselect": "4.0.0",
        "styled-components": "^5.1.1"
    },
    "devDependencies": {
        "@babel/cli": "7.4.3",
        "@babel/core": "7.4.3",
        "@babel/plugin-proposal-class-properties": "7.4.0",
        "@babel/plugin-syntax-dynamic-import": "7.2.0",
        "@babel/plugin-transform-modules-commonjs": "7.4.3",
        "@babel/plugin-transform-react-constant-elements": "7.2.0",
        "@babel/plugin-transform-react-inline-elements": "7.2.0",
        "@babel/preset-env": "7.4.3",
        "@babel/preset-react": "7.0.0",
        "@babel/register": "7.4.0",
        "@testing-library/jest-dom": "*",
        "@testing-library/react": "*",
        "@testing-library/user-event": "*",
        "babel-core": "7.0.0-bridge.0",
        "babel-jest": "^24.9.0",
        "babel-eslint": "10.1.0",
        "babel-loader": "8.1.0",
        "babel-plugin-dynamic-import-node": "2.2.0",
        "babel-plugin-lodash": "3.3.4",
        "babel-plugin-react-intl": "3.0.1",
        "babel-plugin-styled-components": "1.10.0",
        "babel-plugin-transform-react-remove-prop-types": "0.4.24",
        "circular-dependency-plugin": "5.0.2",
        "eslint": "^6.6.0",
        "eslint-config-airbnb": "17.1.0",
        "eslint-config-airbnb-base": "13.1.0",
        "eslint-config-prettier": "4.1.0",
        "eslint-import-resolver-webpack": "0.11.1",
        "eslint-plugin-import": "2.17.2",
        "eslint-plugin-jsx-a11y": "6.2.1",
        "eslint-plugin-prettier": "3.0.1",
        "eslint-plugin-react": "7.12.4",
        "eslint-plugin-react-hooks": "1.6.0",
        "file-loader": "3.0.1",
        "jest-cli": "*",
        "jest-dom": "3.1.3",
        "jest-styled-components": "6.3.1",
        "react-scripts": "*",
        "redux-devtools": "*",
        "redux-devtools-dock-monitor": "*",
        "redux-devtools-log-monitor": "*",
        "redux-logger": "*",
        "rimraf": "2.6.3",
        "webpack": "4.42.0",
        "webpack-cli": "*",
        "webpack-dev-middleware": "3.6.2",
        "webpack-hot-middleware": "2.24.3",
        "webpack-pwa-manifest": "*",
        "whatwg-fetch": "3.0.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 2 versions",
            "> 1%",
            "IE 10"
        ]
    },
    "lint-staged": {
        "*.js": [
            "npm run lint:eslint:fix",
            "git add --force"
        ],
        "*.json": [
            "prettier --write",
            "git add --force"
        ]
    },
    "pre-commit": "lint:staged",
    "resolutions": {
        "babel-core": "7.0.0-bridge.0"
    }
}
